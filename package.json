{
  "name": "tanstarter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 8088",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "format": "biome format --write",
    "lint": "biome lint",
    "check:fix": "biome check --write",
    "check-types": "tsc --noEmit",
    "check": "biome check && bun check-types",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:push": "bun --env-file=.env drizzle-kit push",
    "deps": "bunx taze@latest -Ilw",
    "deps:major": "bunx taze@latest major -Ilw",
    "ui": "bunx shadcn@latest",
    "auth:secret": "bunx @better-auth/cli@latest secret",
    "auth:generate": "bunx @better-auth/cli@latest generate --config ./src/lib/auth/auth.ts --y --output ./src/lib/db/schema/auth.schema.ts && biome format --write ./src/lib/db/schema/auth.schema.ts"
  },
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "@t3-oss/env-core": "0.13.10",
    "@tabler/icons-react": "3.35.0",
    "@tanstack/react-query": "5.90.16",
    "@tanstack/react-router": "1.144.0",
    "@tanstack/react-router-ssr-query": "1.144.0",
    "@tanstack/react-start": "1.145.3",
    "@vitejs/plugin-react": "5.1.2",
    "better-auth": "1.4.9",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.45.1",
    "lucide-react": "0.562.0",
    "nitro": "npm:nitro-nightly@latest",
    "postgres": "3.4.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "shadcn": "3.6.2",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "uuid": "13.0.0",
    "vite": "8.0.0-beta.5",
    "zod": "4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/devtools-vite": "0.4.0",
    "@tanstack/react-devtools": "0.9.0",
    "@tanstack/react-query-devtools": "5.91.2",
    "@tanstack/react-router-devtools": "1.144.0",
    "@types/bun": "1.3.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "0.31.8",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  }
}
